<Window x:Class="file_renamer.reportdialog"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        Title="reportdialog" Height="300" Width="400">
    <Grid>
        <Grid.RowDefinitions>
            <RowDefinition Height="30"></RowDefinition>
            <RowDefinition Height="30"></RowDefinition>
        </Grid.RowDefinitions>
        <Grid.ColumnDefinitions>
            <ColumnDefinition Width="64"></ColumnDefinition>
            <ColumnDefinition Width="*"></ColumnDefinition>
        </Grid.ColumnDefinitions>
        <Label Content="Success: " Grid.Row="0" Grid.Column="0"/>
        <Label Content="0" Grid.Row="0" Grid.Column="1" />
        <Label Content="Errors: "  Grid.Row="1" Grid.Column="0"/>
        <Label Content="0"  Grid.Row="1" Grid.Column="1" />
    </Grid>
</Window>
